<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>All users data</title>
    <style>
        table,tr,td{
            border: 2px solid black;
        }
    </style>
</head>
<body>
    <table>
        <tr>
            <th>ID</th>
            <th>E-MAIL</th>
            <th>USERNAME</th>
           
        </tr>
        <% for(res of result){ %>
            <tr>
                <td><%= res.id %></td>
                <td><%= res.email %></td>
                <td><%= res.username %></td>

                <td><form method="get"  action="/user/<%=res.id%>/edit">
                    <button>Edit username</button>


                </form></td>
            </tr>
            <% } %>
    </table>

    
</body>
</html> 

